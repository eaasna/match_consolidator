{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/../debug/bin/consolidate",
            "args": ["-i", "${fileDirname}/../test/data/dream_stellar/joined_e0.05.gff", "-o", "${fileDirname}/../test/data/dream_stellar/e0.05.gff", "--overlap", "50", "-e", "0.05"],

            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
      ]
    }